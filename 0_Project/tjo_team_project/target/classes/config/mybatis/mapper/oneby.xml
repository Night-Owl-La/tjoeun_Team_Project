<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="oneby">
	<select id="oneby_List" parameterType="int" resultType="vo.OnebyVo">
		select * from oneby where user_idx=#{ user_idx } order by oneby_regdate desc
	</select>
</mapper>


