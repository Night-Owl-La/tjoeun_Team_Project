<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="screen">
	<select id="screen_List" resultType="vo.ScreenVo">
		select * from screen
	</select>
	
	<select id="screen_One" parameterType="int" resultType="vo.ScreenVo">
		select * from screen where scr_idx = #{ scr_idx }
	</select>
	
	
</mapper>


