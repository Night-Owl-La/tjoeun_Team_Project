<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="screen">
	<select id="screen_List" resultType="vo.ScreenVo">
		select * from screen
	</select>
	
	<select id="screen_One_scr_idx" parameterType="int" resultType="vo.ScreenVo">
		select * from screen where scr_idx = #{ scr_idx }
	</select>
	
	<select id="screen_One_tht_idx" parameterType="int" resultType="vo.ScreenVo">
		select * from screen where tht_idx = #{ tht_idx }
	</select>
	
	<select id="screen_One_DOCID" parameterType="String" resultType="vo.ScreenVo">
		select * from screen where docid = #{ DOCID }
	</select>
	
	
</mapper>


