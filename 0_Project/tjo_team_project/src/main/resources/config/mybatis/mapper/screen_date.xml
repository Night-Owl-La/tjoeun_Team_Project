<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="screenDate">
	<select id="screenDate_List" resultType="vo.ScreenDateVo">
		select * from screen_date
	</select>
	
	<select id="screenDate_One" parameterType="String" resultType="vo.ScreenDateVo">
		select * from screen_date where docid = #{ DOCID }
	</select>
	
	
</mapper>


