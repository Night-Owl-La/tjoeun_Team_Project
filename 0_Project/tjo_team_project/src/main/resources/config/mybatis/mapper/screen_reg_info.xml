<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="screenRegInfo">
	<select id="screenRegInfo_List" resultType="vo.ScreenRegInfoVo">
		select * from screen_reg_info
	</select>
	
	<select id="screenRegInfo_One" parameterType="String" resultType="vo.ScreenRegInfoVo">
		select * from screen_reg_info where docid = #{ DOCID }
	</select>
	
	
</mapper>


