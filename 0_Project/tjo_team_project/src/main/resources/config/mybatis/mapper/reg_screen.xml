<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="regScreen">
	<select id="regScreen_List" resultType="vo.RegScreenVo">
		select * from regScreen
	</select>
	
	<insert id="regScreen_Insert" parameterType="vo.RegScreenVo">
		insert into regScreen values(#{docid}, #{reg_title}, #{reg_rating}, #{reg_price})
	</insert>
</mapper>


